{
  "name": "bzilee/multichannel-log-notification",
  "description": "A Laravel package for sending log notifications via multiple channels (Telegram, HTTP, SMS, Email)",
  "type": "laravel-package",
  "require": {
    "php": "^8.0",
		"illuminate/support": "^11.0",
		"illuminate/queue": "^11.0",
		"illuminate/notifications": "^11.0",
		"laravel/vonage-notification-channel": "^3.0",
		"guzzlehttp/guzzle": "^7.0",
		"monolog/monolog": "^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Bzilee\\MultichannelLog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bzilee\\MultichannelLog\\MultichannelLogServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "B.zile TANKEU",
      "email": "b.ziletankeu@gmail.com"
    }
  ]
}
